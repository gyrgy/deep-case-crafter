{
  "name": "deep-case-crafter",
  "version": "0.0.4",
  "description": "Transforms deeply nested object, array, Map, and Set keys between common case formats while preserving TypeScript type safety",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gyrgy/deep-case-crafter.git"
  },
  "bugs": {
    "url": "https://github.com/gyrgy/deep-case-crafter/issues"
  },
  "homepage": "https://github.com/gyrgy/deep-case-crafter#readme",
  "author": "gyrgy",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "prepare": "husky",
    "test": "jest",
    "test-coverage": "npm test -- --coverage",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix --max-warnings=0 --no-cache",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "benchmark": "tsx benchmark/*.benchmark.ts"
  },
  "devDependencies": {
    "@types/benchmark": "2.1.5",
    "benchmark": "2.1.4",
    "@eslint/js": "9.21.0",
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.11",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "jest": "29.7.0",
    "rollup": "4.28.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-visualizer": "5.12.0",
    "ts-jest": "29.2.3",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "typescript-eslint": "8.24.1",
    "tsx": "4.19.3"
  }
}
